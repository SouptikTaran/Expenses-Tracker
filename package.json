{
  "name": "expense-tracker",
  "version": "1.0.0",
  "description": "![alt text](image.png)",
  "main": "index.js",
  "scripts": {
    "start": "npm run build-frontend && concurrently \"npm run serve-frontend\" \"npm run start-backend\"",
    "build-frontend": "cd frontend && npm run build",
    "serve-frontend": "serve -s frontend/dist -l 5000",  
    "start-backend": "cd backend && npm start",
    "dev": "concurrently \"npm run dev-frontend\" \"npm run dev-backend\"",
    "dev-frontend": "cd frontend && npm run dev",
    "dev-backend": "cd backend && npm run dev"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.0.1",
    "serve": "^14.2.4"
  }
}
